{
  "tags": {
    "allowUnknownTags": true
  },
  "source": {
    "exclude": [
      "node_modules/",
      "docs/"
    ]
  },
  "plugins": [
    "plugins/markdown"
  ],
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": true,
    "useLongnameInNav": false,
    "default": {
      "includeDate": false
    }
  },
  "opts": {
    "destination": "./docs/",
    "encoding": "utf8",
    "private": true,
    "recurse": true,
    "template": "./node_modules/minami"
  }
}
